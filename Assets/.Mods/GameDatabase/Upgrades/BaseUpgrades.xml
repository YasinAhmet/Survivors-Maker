<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <UpgradeDef Name="DamageIncreaseFL">
        <description>Damage Increase for leader. %25 bonus to raw damage.</description>
        <behaviours>
            <behaviour Name="IncreaseStat">
                <customParameters>
                    <parameter Name="StatName">
                        <value>Damage</value>
                    </parameter>
                    <parameter Name="BonusRate">
                        <value>1.25</value>
                    </parameter>
                </customParameters>
            </behaviour>
        </behaviours> 
    </UpgradeDef>
</Defs>